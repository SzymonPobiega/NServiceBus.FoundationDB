    <Dependency Name="NLog" Version="2.0.1.2" Mode="Fixed" />
    <Dependency Name="NServiceBus" Version="4.4.2" Mode="Float" />    
    <Dependency Name="NServiceBus.Interfaces" Version="4.4.2" Mode="Float" />
    <Dependency Name="NUnit" Version="2.6.2" Mode="Fixed" />    
